<jsp:root
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:c="http://java.sun.com/jsp/jstl/shept"
	xmlns:spring="http://www.springframework.org/tags"
	xmlns:shept-core="http://www.shept.org/tags/shept-core"
	version="2.0"
>

	<jsp:directive.attribute name="message" required="true"
		description="Name of the form input model attribute"
	/>

	<shept-core:includeJavaScript />
		
	<div id="dialog-confirm">
		<p>
			${message}
		</p>
	</div>

</jsp:root>	
